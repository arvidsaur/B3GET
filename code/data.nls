to collect-data
  
  let verification? false   ; TRUE = runs periodic code verification checks
  let conduct-census? true  ; TRUE = records every animal upon its death into a census
  let record-group? false   ; TRUE = records every group upon its creation into a census
  let sim-scans? true       ; TRUE = scans the world and population level phenomena
  let animal-scans? false   ; TRUE = scans all animals every 100 timesteps
  let group-scans? false    ; TRUE = scans all groups every 100 timesteps
  let focal-follows? false  ; TRUE = small chance to record an individual's entire life
  
  ;----------------------------------------------------------------------------------------------     
  ; SIMULATIONS
  ;----------------------------------------------------------------------------------------------
  
  if ( ticks = 1 ) [ crt 1 [ record-simulation die ]]
  
  ;----------------------------------------------------------------------------------------------     
  ; VERIFICATION
  ;----------------------------------------------------------------------------------------------
  
  if ( verification? = true and random-float 1.0 < 0.0001 ) [ save-verification-to "verification.csv" ]
  
  ;----------------------------------------------------------------------------------------------     
  ; SCANS
  ;----------------------------------------------------------------------------------------------
  
  if ( ticks > 0 and ceiling (ticks / 100) = (ticks / 100) ) 
  [ 
    if ( sim-scans? ) [ scan-simulation ]
    if ( group-scans? and any? groups ) [ ask groups [ scan-group ]]
    if ( animal-scans? and any? anima1s ) [ ask anima1s with [ generation-number > 0 ] [ scan-anima1 ]]
  ]
  
  ;----------------------------------------------------------------------------------------------     
  ; CENSUS
  ;----------------------------------------------------------------------------------------------
  
  if ( conduct-census? ) [ ask anima1s with [ ticks-at-death > 0 and ticks = ( ticks-at-death + 1 ) and generation-number > 0 ] [ record-anima1 ] ]
  if ( record-group? ) [ ask groups with [ age = 0 ] [ record-group ]]
  
  ;----------------------------------------------------------------------------------------------     
  ; FOCAL FOLLOWS
  ;----------------------------------------------------------------------------------------------
  
  ; There is a small chance to starting "following" an agent if they were born this timestep 
  if ( focal-follows? = true and subject = nobody ) [ if ( any? anima1s with [ age = 0 and generation-number > 0 ] ) [ follow one-of anima1s with [ age = 0 and generation-number > 0 ] ]]
  
  ; If currently "following" an agent, take records of this subject
  if ( focal-follows? = true and subject != nobody ) [ ask subject [ scan-focal ] ]
  
end

